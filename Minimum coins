class Solution{
	public:
	int MinimumCoins(vector<int>& coins, int amount) {
        int n = coins.size();
        int cnt=0;
        for (int i=n-1;i>=0;i--)
        {
            while (amount>=coins[i])
            {
                amount-=coins[i];
                cnt++;
            }
        }
        if (amount>0)
        return -1;
        else
        return cnt;
    }
};
